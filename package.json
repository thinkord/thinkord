{
  "name": "thinkord",
  "version": "0.1.0",
  "description": "Electron application dedicated to helping people comveniently capture their thoughts.",
  "contributors": [
    "Lo Tingyi",
    "Lu Jiaqi",
    "Chiu Shihchuan"
  ],
  "private": true,
  "homepage": "./",
  "main": "public/electron.js",
  "keywords": [
    "Electron",
    "React",
    "note"
  ],
  "license": "GNU GPLv3",
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test",
    "react-eject": "react-scripts eject",
    "electron-build": "webpack --config ./webpack.config.js",
    "start": "concurrently \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && npm run electron-build && electron .\"",
    "lint": "eslint **/*.{js,ts,jsx} --quiet --fix ",
    "build": "npm run electron-build && npm run react-build",
    "pack": "electron-builder --config electron-builder.json"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/bluebird": "^3.5.32",
    "@types/dom-mediacapture-record": "^1.0.7",
    "@types/node": "^14.11.2",
    "@types/sqlite3": "^3.1.6",
    "@types/validator": "^13.1.0",
    "bootstrap": "^4.5.2",
    "cross-env": "^7.0.2",
    "electron-is-dev": "^1.2.0",
    "loglevel": "^1.7.0",
    "node-sass": "^4.14.1",
    "pg-hstore": "^2.3.3",
    "react": "^16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-contextmenu": "^2.14.0",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.3.5",
    "sequelize-typescript": "^1.1.0",
    "sqlite3": "^5.0.0",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.2.0",
    "electron": "^11.0.3",
    "electron-builder": "^22.8.0",
    "electron-rebuild": "^2.2.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "prettier": "^2.1.2",
    "ts-loader": "^8.0.4",
    "typescript": "^4.0.3",
    "wait-on": "^5.1.0",
    "webpack-cli": "^3.3.12"
  }
}
